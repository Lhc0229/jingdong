  height: 100rpx;
}
/*商品列表*/
.goods_list{
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.goods_list image{
  width: 340rpx;
}
.goods_list view{
  background-color: #fff;
  border-radius: 12rpx;
  margin-bottom: 20rpx;
}
.goods_list text{
  width: 340rpx;
  box-sizing: border-box;
}
.goods_list text:nth-child(2){
  font-size: 24rpx;
  padding: 0 16rpx;
  text-align: justify;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.goods_list text:nth-child(3){
  color: red;
  box-sizing: border-box;
  padding: 16rpx 20rpx;
}